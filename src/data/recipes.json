    {
        "id": 1,
        "name": "Shakshuka",
        "desc": "Eggs dish with tomato",
        "img": "https://downshiftology.com/wp-content/uploads/2015/11/shakshuka-12.jpg",
        "duration": 15,
        "difficultyLevel": 2,
        "type": "Eggs",
        "userId": 1,
        "ingredients": [
            {
                "ingredientId": 1,
                "index": 1,
                "name": "Egg",
                "units": "x",
                "quantity": 4
            },
            {
                "ingredientId": 2,
                "index": 2,
                "name": "Tameto",
                "units": "x",
                "quantity": 5
            },
            {
                "ingredientId": 3,
                "index": 3,
                "name": "Onion",
                "units": "x",
                "quantity": 2
            },
            {
                "ingredientId": 4,
                "index": 4,
                "name": "Garlic",
                "units": "heads",
                "quantity": 1
            },
            {
                "ingredientId": 5,
                "index": 5,
                "name": "Sweet Paprica Power",
                "units": "tablespoon",
                "quantity": 3
            },
            {
                "ingredientId": 6,
                "index": 6,
                "name": "Hot Paprica Powder",
                "units": "teaspoon",
                "quantity": 1
            },
            {
                "ingredientId": 7,
                "index": 7,
                "name": "White Pepper Powder",
                "units": "teaspoon",
                "quantity": 1
            },
            {
                "ingredientId": 9,
                "index": 9,
                "name": "Black Pepper Powder",
                "units": "teaspoon",
                "quantity": 1
            },
            {
                "ingredientId": 8,
                "index": 8,
                "name": "Table Salt",
                "units": "teaspoon",
                "quantity": 0.5
            }
        ],
        "preparationSteps": [
            {
                "stepNumber": 1,
                "description": "Cut the tametos to small cube 1cm x 1cm."
            },
            {
                "stepNumber": 2,
                "description": "Pill and cut the onions into small cubes 1cm x 1cm."
            },
            {
                "stepNumber": 3,
                "description": "Pill and cut the garlic into thin slices."
            },
            {
                "stepNumber": 4,
                "description": "Hit a pan with 3 tablespoons of vegetalbe oil."
            },
            {
                "stepNumber": 5,
                "description": "When the oil is smoking add the onion and fry on high flame until it is transparent."
            },
            {
                "stepNumber": 6,
                "description": "Add the garlic and fry for about 2 min on high flame."
            },
            {
                "stepNumber": 7,
                "description": "Add the tameto cubes into the pan, mix with the onion and garlic, let it cook for a minute."
            },
            {
                "stepNumber": 8,
                "description": "Add 2 table spoons of vegetable oil, add the spices to your liking and stir everything together."
            },
            {
                "stepNumber": 9,
                "description": "Recommended spices: Sweet and Hot Paprika, garlic powder, white pepper, kosher salt."
            },
            {
                "stepNumber": 10,
                "description": "Do the next step for each egg seperately one at a time, arranging them in the pan:"
            },
            {
                "stepNumber": 11,
                "description": "Break the egg into a small bowl, check there is not eggshell in the bowl and add it to the pan."
            },
            {
                "stepNumber": 12,
                "description": "Lower the flame to low-medium, add vegetable oil if needed and Cover the pan."
            },
            {
                "stepNumber": 13,
                "description": " Let it cook for 10-15 min."
            },
            {
                "stepNumber": 14,
                "description": "Lift the cover off the pan, check the eggs are cooked to your liking."
            },
            {
                "stepNumber": 15,
                "description": "Turn off the flame and serve to the table with pita bread or toasts."
            }
        ]
    },
